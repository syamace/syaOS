{
  "name": "syaos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' vite --port ${PORT:-5173}",
    "dev:vercel": "vercel dev",
    "version:bump": "bun run scripts/generate-build-version.ts --bump",
    "prebuild": "bun run scripts/generate-build-version.ts",
    "build": "tsc -b && vite build && cp dist/sw.js dist/version.json .vercel/output/static/ && (cp dist/workbox-*.js .vercel/output/static/ 2>/dev/null || true) && node -e \"const c=require('./.vercel/output/config.json');c.cleanUrls=true;c.trailingSlash=false;require('fs').writeFileSync('./.vercel/output/config.json',JSON.stringify(c,null,2))\"",
    "build:tauri": "bun run scripts/build-tauri.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:icons": "bun run scripts/generate-icon-manifest.ts",
    "generate:wallpapers": "bun run scripts/generate-wallpaper-manifest.ts",
    "generate:app-docs": "bun run scripts/generate-app-docs.ts",
    "generate:changelog": "bun run scripts/generate-changelog.ts",
    "generate:docs": "bun run scripts/generate-app-docs.ts && bun run scripts/generate-changelog.ts && bun run scripts/generate-docs.ts",
    "launch-kiosk": "bash scripts/launch-kiosk.sh",
    "i18n:extract": "bun run scripts/extract-strings.ts",
    "i18n:sync": "bun run scripts/sync-translations.ts",
    "i18n:sync:dry-run": "bun run scripts/sync-translations.ts --dry-run",
    "i18n:sync:mark-todo": "bun run scripts/sync-translations.ts --mark-untranslated",
    "i18n:find-untranslated": "bun run scripts/find-untranslated-strings.ts",
    "i18n:translate": "bun run scripts/machine-translate.ts",
    "i18n:translate:dry-run": "bun run scripts/machine-translate.ts --dry-run",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:mac": "bash -c 'set -a && [ -f .env.local ] && source .env.local; set +a && tauri build --target universal-apple-darwin'",
    "tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "seed:dev": "bun run scripts/seed-dev-users.ts",
    "test": "bun run tests/run-all-tests.ts",
    "test:api": "bun run tests/run-all-tests.ts",
    "test:chat-rooms": "bun run tests/test-chat-rooms.ts",
